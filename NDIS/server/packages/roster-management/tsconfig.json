{
  "compilerOptions": {
  "target": "ES2020",
  "module": "commonjs",
  "outDir": "dist",
  "rootDir": "src",
  "declaration": true,
  "emitDeclarationOnly": false,
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "composite": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
